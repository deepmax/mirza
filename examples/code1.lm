func factorial(n : int) : int {
    if n <= 1 {
        ret 1
    }
    ret factorial(n - 1) * n
}


func max(a: int, b: int): int {
    if a > b {
        ret a
    } else {
        ret b
    }
}

var a = factorial(3)
var b = factorial(4)

for var i = 0; i < max(a, b); i = i + 1 {
    print i
    print " "
    print a + b
    print "\n"

    if i == 2 {
        break
    }
}
