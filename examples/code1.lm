func factorial(n : int) : int
{
    if n <= 1 {
        ret 1
    }
    ret factorial(n - 1) * n
}
print factorial(3)

func max(a: int, b: int): int
{
    if a > b
    {
        ret a
    }
    else
    {
        ret b
    }
}

